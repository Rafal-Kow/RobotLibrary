GLOBAL DEF VW_SGB_CONFIG_SPOT_CHECK(GunNumber :IN, CompType :IN)
   DECL INT GunNumber
   DECL COMPTYP_T CompType 
   IF (ServoGun[GunNumber].CompType <> CompType) THEN  
      LOOP 
         MsgQuit("ServoGunConfigChangedCompType","VwInlineForms",,,93)
         HALT
      ENDLOOP
   ENDIF
